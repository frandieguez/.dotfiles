{
  "workbench.colorTheme": "Nord",
  "yaml.schemas": {
    "file:///home/fran/.vscode-insiders/extensions/atlassian.atlascode-2.4.4/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "'Hasklig',  'Courier Prime', 'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.sideBar.location": "left",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontLigatures": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 120],
  "explorer.confirmDelete": false,
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "git.fetchOnPull": true,
  "gitlens.currentLine.enabled": false,
  "php.suggest.basic": false,
  "php.validate.executablePath": "/usr/bin/php",
  "search.useIgnoreFiles": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontSize": 13,
  "window.title": "${appName}${separator}${rootName}${separator}${dirty}${activeEditorLong}${separator}",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.visible": true,
  "window.menuBarVisibility": "default",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.easymotion": true,
  "vim.leader": "<Space>",
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    // Clean search hightlight with C-n
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    // Undo with u
    {
      "before": ["u"],
      "after": [],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    // Redo with C-r
    {
      "before": ["<C-r>"],
      "after": [],
      "commands": [
        {
          "command": "redo"
        }
      ]
    },
    {
      "before": ["<leader>", "m"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["bookmarks.list"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": [":w"]
    }
  ],
  "vim.incsearch": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.surround": true,
  "vim.useSystemClipboard": true,
  "vim.enableNeovim": true,
  "workbench.colorCustomizations": {
    "editorWarning.foreground": "#FFAA00"
    // "editor.background": "#ff0000"
  },
  "emmet.triggerExpansionOnTab": true,
  "breadcrumbs.enabled": false,
  "vim.neovimPath": "/usr/bin/nvim",
  "editor.lightbulb.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.fontFamily": "'Hack', monospace",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.smoothScrolling": true,
  "files.trimFinalNewlines": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "python.insidersChannel": "off",
  "atlascode.bitbucket.explorer.relatedJiraIssues.enabled": false,
  "atlascode.jira.enabled": false,
  "atlascode.jira.statusbar.enabled": false,
  "atlascode.bitbucket.statusbar.enabled": false,
  "liveshare.showInStatusBar": "whileCollaborating",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": ["apikey"],
  "cSpell.enabled": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.formatOnSave": true,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter-notebook",
      "filenamePattern": "*.ipynb"
    }
  ],
  "jupyter.insidersChannel": "off",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.showStartPage": false
}
