{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontFamily": "'Monaspace Neon', monospace, 'Cascadia Mono', 'JuliaMono', 'Hasklig',  'Courier Prime', 'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  // "editor.fontLigatures": true,
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'calt', 'dlig'",
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    120
  ],
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDelete": false,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.fetchOnPull": true,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.maxItemsComputed": 15000,
  // "json.sortOnSave.enable": true,
  "php.suggest.basic": false,
  "remote.localPortHost": "allInterfaces",
  "search.exclude": {
    "**/build": true
  },
  "search.useIgnoreFiles": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "'Hack', monospace",
  "terminal.integrated.fontSize": 13,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-p>": false
  },
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": [
        "<Esc>"
      ],
      "before": [
        "j",
        "j"
      ]
    }
  ],
  "vim.leader": "<Space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    // Clean search hightlight with C-n
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "after": [
        "d",
        "d"
      ],
      "before": [
        "<leader>",
        "d"
      ]
    },
    // Undo with u
    {
      "after": [],
      "before": [
        "u"
      ],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    // Redo with C-r
    {
      "after": [],
      "before": [
        "<C-r>"
      ],
      "commands": [
        {
          "command": "redo"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "m"
      ],
      "commands": [
        "bookmarks.toggle"
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "bookmarks.list"
      ]
    },
    {
      "before": [
        "<leader>",
        "q"
      ],
      "commands": [
        ":q"
      ]
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        ":w"
      ]
    }
  ],
  "vim.surround": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    }
  ],
  "window.menuBarVisibility": "classic",
  // "window.title": "${appName}${separator}${rootName}${separator}${dirty}${activeEditorLong}${separator}",
  "window.titleBarStyle": "custom",
  "workbench.colorCustomizations": {
    "[GitHub Dark]": {},
    "[GitHub Light]": {
      "window.activeBorder": "#ccc"
    }
  },
  "workbench.editor.decorations.badges": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "eq-material-theme-icons",
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "workbench.preferredLightColorTheme": "GitHub Light",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.settings.applyToAllProfiles": [
    "workbench.preferredLightColorTheme",
    "workbench.colorTheme",
    "workbench.preferredDarkColorTheme",
    "workbench.iconTheme",
    "editor.cursorStyle",
    "editor.fontFamily",
    "editor.fontSize",
    "files.autoSave",
    "editor.bracketPairColorization.independentColorPoolPerBracketType",
    "editor.bracketPairColorization.enabled",
    "editor.guides.bracketPairs",
    "editor.lightbulb.enabled",
    "editor.linkedEditing",
    "editor.renderWhitespace",
    "editor.rulers",
    "editor.smoothScrolling",
    "editor.cursorSmoothCaretAnimation",
    "editor.cursorBlinking",
    "editor.stickyScroll.enabled",
    "editor.formatOnSave",
    "editor.fontLigatures",
    "editor.minimap.enabled",
    "editor.inlineSuggest.enabled",
    "editor.suggestSelection",
    "files.trimFinalNewlines",
    "files.trimTrailingWhitespace",
    "workbench.productIconTheme",
    "workbench.activityBar.visible",
    "workbench.colorCustomizations",
    "workbench.editor.decorations.badges",
    "workbench.editor.highlightModifiedTabs",
    "window.autoDetectColorScheme",
    "search.useIgnoreFiles",
    "terminal.integrated.cursorBlinking",
    "terminal.integrated.cursorStyle",
    "terminal.integrated.fontFamily",
    "terminal.integrated.fontSize",
    "terminal.integrated.tabs.enabled",
    "emmet.triggerExpansionOnTab",
    "git.autofetch",
    "git.fetchOnPull",
    "typescript.updateImportsOnFileMove.enabled",
    "vim.useSystemClipboard",
    "vim.useCtrlKeys",
    "vim.surround",
    "vim.leader",
    "vim.incsearch",
    "vim.hlsearch",
    "vim.highlightedyank.enable",
    "vim.handleKeys",
    "vim.enableNeovim",
    "vim.easymotion",
    "typescript.inlayHints.enumMemberValues.enabled",
    "javascript.updateImportsOnFileMove.enabled",
    "javascript.preferences.importModuleSpecifier",
    "[scss]",
    "[markdown]",
    "[jsonc]",
    "[json]",
    "[javascript]",
    "[html]"
  ],
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "[markdown]": {
    "editor.wordWrap": "on"
  },
  "workbench.colorTheme": "GitHub Dark",
  "redhat.telemetry.enabled": true,
  "workbench.tree.enableStickyScroll": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "window.autoDetectColorScheme": true
}